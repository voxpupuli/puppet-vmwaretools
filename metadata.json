    {
      "operatingsystem": "SLED",
      "operatingsystemrelease": [ "10", "11" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "10.04", "10.10", "11.04", "11.10", "12.04" ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.0.0 <5.0.0"
    }
  ]
