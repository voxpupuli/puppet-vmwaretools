{
  "name": "razorsedge-vmwaretools",
  "version": "6.1.0",
  "author": "razorsedge",
  "summary": "Install VMware Tools Operating System Specific Packages (OSP) on VMware guests.",
  "license": "Apache-2.0",
  "project_page": "https://github.com/razorsedge/puppet-vmwaretools",
  "issues_url": "https://github.com/razorsedge/puppet-vmwaretools/issues",
  "tags": [ "virtualization", "vmware", "vmware-tools", "vmware_tools", "vmtools" ],
  "source": "https://github.com/razorsedge/puppet-vmwaretools.git",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=2.3.0 <5.0.0"
    },
    {
      "name": "puppet/zypprepo",
      "version_requirement": ">=2.0.0 <3.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">=2.0.0 <3.0.0"
    },
    {
      "name": "wolfspyre/vmware_puppetfact",
      "version_requirement": ">=0.2.1 <2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "5", "6" ]
